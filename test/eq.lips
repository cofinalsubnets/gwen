(test equality
 (: t (~())
    (eq-mtx x a b)
     (= x (= a b)
      ((\ x (= a x x x)) b)
      ((\ x (= x a x x)) b)
      ((\ x (= x x a x)) b)
      ((\ x (= x x x a)) b))
  (= t (eq-mtx t 1 1)
       (eq-mtx () 1 0)))
 (= (L 0) (L 0))
 (= (L 0 0) (X 0 (X 0 ())))
 (= 'a 'a)
 (= "a" "a")
 (~ (= "a" "b"))
 (~ (= (ygen) (ygen)))
 (~ (= (tbl) (tbl))))
