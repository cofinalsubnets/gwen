 )

(: (stripPrefix p s) (? (= p (ssub s 0 (slen p))) (ssub s (slen p) (slen s))))
(assert (= "eon" (stripPrefix "vapor" "vaporeon")))
(: (lit l y n s) ((\ ss (? ss (y ss (X l 0)) (n s))) (stripPrefix l s)))
